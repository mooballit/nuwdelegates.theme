<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="nuwdelegates.theme"
    purge="False">

    <configlet
        title="Login Registry"
        action_id="nuwdelegates.theme.login_tracking.logins_report"
        appId="nuwdelegates.theme"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@logins-overview"
        icon_expr="string:${portal_url}/++theme++nuw.types/apitool.png"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>
    
    <configlet
        title="Print Registry"
        action_id="nuwdelegates.theme.login_tracking.prints_report"
        appId="nuwdelegates.theme"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@prints-overview"
        icon_expr="string:${portal_url}/++theme++nuw.types/apitool.png"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>

</object>
